@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Kiloh Diagnostics</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css"> 
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>  
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    
</head>
<body>
    <div>
        <nav class="navbar navbar-custom navbar-fixed-top">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="#" class="navbar-brand">Kiloh</a>
            </div>
            <!-- Collection of nav links, forms, and other content for toggling -->
            <div id="navbarCollapse" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    @*<li><a href="#">Queue Monitor</a></li>*@
                    <li>@Html.ActionLink("Queue Monitor", "Index", "QueueMonitor")</li>
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">About <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Kiloh Queue Monitor (v1)</a></li>
                            <li><a href="#">Examples Of:</a></li>
                            <li><a href="#">MVC5</a></li>
                            <li><a href="#">JQuery</a></li>
                            <li><a href="#">Custom server side paging and sorting</a></li>
                            <li><a href="#">Bootstrap</a></li>
                            <li><a href="#">T4 Template Generation (EF classes, IContext, Crud Repositories)</a></li>
                            <li><a href="#">Query Objects</a></li>
                            <li><a href="#">Mediator Pattern usage</a></li>
                            <li><a href="#">Ninject Dependency Injection</a></li>
                            <li><a href="#">Custom Primary Key Generation with EF</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </div>


<div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; Kiloh Software</p>
        </footer>
    </div>

    
</body>
</html>